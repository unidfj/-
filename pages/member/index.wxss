.header {
	width: 355*2rpx;
	height: 177*2rpx;
	height: 300rpx;
	background: var(--basecolor);
	margin: 0 20rpx;
	border-radius: 16rpx;
}

.userimg {
	width: 160rpx;
	height: 160rpx;
	overflow: hidden;
	border-radius: 50%;
	margin: 0 20rpx;
}

.userdata {
	height: 50%;
	flex: 1;
	display: flex;
	flex-direction: column;
	justify-content: space-between;
	font-size: 16px;
	color: #fff;
}

.username text {
	font-size: 14px;
	color: rgba(253, 204, 8, 1);
}

.cellclass {
	margin: 30rpx 20rpx;
	border: 4px solid #ccc;
	border-radius: 15rpx;
	overflow: hidden;
	box-shadow: 0px 0px 3px 0px rgba(0, 0, 0, 0.4);
}

.overlay {
	height: 100vh;
	width: 100vw;
}

.overlayitem {
	z-index: 999;
	flex: 1;
	height: 300rpx;
	/* border-radius: 5rpx; */
	overflow: hidden;
	padding: 0 10rpx;
}