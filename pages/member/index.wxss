page {
	box-sizing: border-box;
	padding: 30rpx;
}

.header {
	position: relative;
	width: 690rpx;
	height: 200rpx;
	margin: 0 auto;
	background: linear-gradient(180deg, rgba(250, 125, 90, 1) 0%, rgba(251, 99, 84, 1) 100%);
	border-radius: 10rpx;
}

.userimg {
	width: 125rpx;
	height: 125rpx;
	overflow: hidden;
	border-radius: 50%;
	margin-left: 40rpx;
	margin-right: 20rpx;
}

.userdata {
	height: 50%;
	flex: 1;
	display: flex;
	flex-direction: column;
	justify-content: space-between;
	text-align: left;
	font-size: 32rpx;
	color: #fff;
}

.norenzheng {
	position: absolute;
	right: 0;
	top: 50%;
	transform: translate(0, -50%);
	width: 140rpx;
	height: 60rpx;
	background: rgba(255, 255, 255, 1);
	box-shadow: 0rpx 2rpx 4rpx 0rpx rgba(178, 27, 0, 0.49);
	border-radius: 100rpx 0rpx 0rpx 100rpx;
	font-family: PingFangSC-Medium, PingFang SC;
	font-weight: 600;
	color: rgba(251, 98, 84, 1);
}

.cellclass {
	margin-top: 30rpx;
	width: 690rpx;
	height: 400rpx;
	background: rgba(255, 255, 255, 1);
	border-radius: 10px;
	display: flex;
	flex-direction: column;
	justify-content: center;
	align-items: center;
	overflow: hidden;
}

.cellitem {
	width: 90%;
	height: 25%;
	flex-shrink: 0;
	border-bottom: 1px solid #ccc;
}

.cellitem .icon {
	width: 50rpx;
	height: 50rpx;
}

.cellitem .title {
	flex: 1;
	padding-left: 30rpx;
}



.overlay {
	height: 100vh;
	width: 100vw;
}

.overlayitem {
	margin: 80rpx auto;
	z-index: 999;
	overflow: hidden;
	box-sizing: border-box;
	padding: 80rpx;
	width: 350rpx;
	height: 450rpx;
	background: #fff;
	border-radius: 8rpx;
	font-size: 36rpx;
	font-family: PingFangSC-Medium, PingFang SC;
	font-weight: 600;
	text-align: center;
	color: rgba(251, 98, 84, 1);
}

.overlayitem:nth-child(2) {
	color: rgba(44, 125, 255, 1);
}

.imgbox {
	width: 180rpx;
	height: 180rpx;
	border-radius: 50%;
	background: #fff;
	overflow: hidden;
	margin-bottom: 65rpx;
}