.yue {
	display: flex;
	justify-content: right;
	margin: 16rpx 60rpx 4rpx 0;
	padding-left: 500rpx;
}

.yue .num {
	color: rgba(16, 16, 16, 1);
	font-size: 28rrpx;
	text-align: left;
	font-family: PingFangSC-regular;
	color: var(--basecolor);
}

.tab {
	width: 100%;
	height: 92rpx;
	border: 2rpx solid rgba(187, 187, 187, 1);
}

.tab .tabitem {
	flex-shrink: 0;
	flex: 1;
	text-align: center;
}

.detail {
	height: 120rpx;
	padding: 0 18rpx;
	box-sizing: border-box;
	border-bottom: 1px solid rgba(187, 187, 187, 1);
}

.detail view:nth-child(1) {
	width: 220rpx;
}

.detail view:nth-child(2) {
	flex: 1;
	padding-left: 20px;
}

.detail view:nth-child(3) {
	width: 300rpx;
}

.detailitem {
	flex-shrink: 0;
	text-align: center;
	white-space: nowrap;
	overflow: hidden;
	text-overflow: ellipsis;
}


.nomore {
	margin: 34rpx auto;
	text-align: center;
	color: rgba(102, 102, 102, 0.82);
	font-family: PingFangSC-regular;
}